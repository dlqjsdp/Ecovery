{{>layouts/header}}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">EMAIL</th>
            <th scope="col">PASSWORD</th>
        </tr>
    </thead>
    <tbody>
        {{! HTML 안에서 변수를 출력하거나 반복/조건을 처리할 수 있도록 도와줌 }}
        {{#memberList}}
        <tr>
            <th>{{id}}</th>
            <th>{{email}}</th>
            <th>{{password}}</th>
        </tr>
        {{/memberList}}
    </tbody>
</table>

<a href="/members">Go to Member List</a>

{{>layouts/footer}}